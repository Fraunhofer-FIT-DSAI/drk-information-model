@prefix rdfs:         <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl:          <http://www.w3.org/2002/07/owl#> .
@prefix lang:         <https://w3id.org/drk/vocabs/languages-iso639-1#> .
@prefix dcterms:      <http://purl.org/dc/terms/> .
@prefix vann:         <http://purl.org/vocab/vann/> .
@prefix schema:       <https://schema.org/> .
@prefix xsd:          <http://www.w3.org/2001/XMLSchema#> .
@prefix ids:          <https://w3id.org/idsa/core/> .
@prefix dcat:         <http://www.w3.org/ns/dcat#> .
@prefix voaf:         <http://purl.org/vocommons/voaf#> .
@prefix skos:         <http://www.w3.org/2004/02/skos/core#> .
@prefix cc:           <http://creativecommons.org/ns#> .
@prefix foaf:         <http://xmlns.com/foaf/0.1/> .
@prefix mo:           <http://purl.org/ontology/mo/> .
@prefix dbpedia:      <http://dbpedia.org/ontology/> .
@prefix event:        <http://purl.org/NET/c4dm/event.owl#> .
@prefix dbo:          <http://dbpedia.org/ontology/> .
@prefix nfo:          <http://www.semanticdesktop.org/ontologies/2007/03/22/nfo#> .
@prefix time:         <http://www.w3.org/2006/time#> .
@prefix ncal:         <http://www.semanticdesktop.org/ontologies/2007/04/02/ncal#> .
@prefix prov:         <http://www.w3.org/ns/prov#> .
@prefix geosparql:    <http://www.opengis.net/ont/geosparql#> .
@prefix geo:          <http://www.w3.org/2003/01/geo/wgs84_pos#> .
@prefix vcard:        <http://www.w3.org/2006/vcard/ns#> .



# Ontology Definition
drk:
    a                             owl:Ontology ;
    rdfs:label                    "DRK Musikschul-Manager Ontology"@en, "DRK Musikschul-Manager Ontologie"@de ;
    rdfs:comment                  "The DRK ontology for Hamburger Konservatorium's Musikschul-Manager, providing semantic concepts and properties for music school management, including student-teacher relationships, lesson scheduling, and administrative processes."@en ;
    dcterms:title                 "DRK Musikschul-Manager Ontology"@en, "DRK Musikschul-Manager Ontologie"@de ;
    owl:versionInfo               "1.0.0"^^xsd:string ;
    dcterms:hasVersion            "1.0.0"^^xsd:string ;
    dcterms:created               "2024-11-14"^^xsd:date ;
    dcterms:modified              "2024-11-14"^^xsd:date ;
    dcterms:issued                "2024-11-14"^^xsd:date ;
    dcterms:creator               [ a                  foaf:Person ;
                                    foaf:name          "Daham Mohammed Mustafa"@en ;
                                    foaf:mbox          <mailto:daham.mustafa@fit.fraunhofer.de> ;
                                    foaf:homepage      <https://github.com/Daham-Mustaf> ;
                                    dcterms:identifier <https://orcid.org/0000-0003-1867-4428> ] ;
    dcterms:creator               [ a                  foaf:Person ;
                                    foaf:name          "Rohit A. Deshmukh"@en ;
                                    foaf:mbox          <mailto:rohit.deshmukh@fit.fraunhofer.de> ;
                                    foaf:homepage      <https://github.com/rohitadeshmukh13> ;
                                    dcterms:identifier <https://orcid.org/0000-0003-2885-7076> ] ;
    dcterms:creator               [ a                  foaf:Person ;
                                    foaf:name          "Christoph Lange-Bever"@en ;
                                    foaf:mbox          <mailto:christoph.lange-bever@fit.fraunhofer.de> ;
                                    foaf:homepage      <https://github.com/clange> ;
                                    dcterms:identifier <https://orcid.org/0000-0001-9879-3827> ] ;
    dcterms:publisher             [ a             foaf:Organization ;
                                    foaf:name     "Fraunhofer FIT" ;
                                    foaf:homepage <https://www.fit.fraunhofer.de/> ] ;
    dcterms:license               <http://creativecommons.org/licenses/by/4.0/> ;
    cc:license                    <http://creativecommons.org/licenses/by/4.0/> ;
    dcterms:rights                "This ontology is distributed under Creative Commons Attribution 4.0 International License"@en ;
    vann:preferredNamespacePrefix "drk" ;
    vann:preferredNamespaceUri    "http://w3id.org/drk/ontology/" ;
    rdfs:seeAlso                  <https://github.com/Fraunhofer-FIT-DSAI/drk-information-model> ;
    vann:example                  <https://github.com/Fraunhofer-FIT-DSAI/drk-information-model/tree/main/examples> ;
    dcterms:description           """
        The DRK Musikschul-Manager ontology provides a comprehensive vocabulary for describing music school operations and management.
        The ontology is designed specifically for the Hamburger Konservatorium's requirements while maintaining interoperability within the DRK framework.
    """@en ; 
    dcterms:language              lang:en, lang:de ;
    dcterms:subject               "Music Education"@en, "School Management"@en, "Cultural Education"@en, "Music School Administration"@en ;
    dcterms:audience              "Music Schools"@en, "Music Teachers"@en, "Music Students"@en, "School Administrators"@en .


# Core Classes
:MusicEducationSchool
   a rdfs:Class, owl:Class ;
   rdfs:isDefinedBy drk: ;
   rdfs:label "Music School"@en ;
   skos:definition "An educational School specialized in music education."@en ;
   rdfs:subClassOf schema:EducationalOrganization .

:MusicSchoolMember
   a rdfs:Class, owl:Class;
   rdfs:isDefinedBy drk: ;
   rdfs:label "Music School Member"@en ;
   skos:definition "A person who interacts with the music school system, including students, teachers, and administrators."@en ;
   rdfs:subClassOf schema:Person .
    
:MusicTeacher
    a rdfs:Class, owl:Class ;
    rdfs:isDefinedBy drk: ;
    rdfs:label "Music Instructor"@en ;
    skos:definition "Professional music educator."@en ;
    rdfs:subClassOf :MusicSchoolMember .

:MusicStudent
    a rdfs:Class, owl:Class ;
    rdfs:isDefinedBy drk: ;
    rdfs:label "Music Learner"@en ;
    skos:definition "Student enrolled for music education."@en ;
    rdfs:subClassOf :MusicSchoolMember .

:MusicCourse
    a rdfs:Class, owl:Class ;
    rdfs:isDefinedBy drk: ;
    rdfs:label "Music Instruction"@en ;
    skos:definition "A cours conducted by a teacher for one or more students, focusing on a specific musical subject or skill."@en ;
    rdfs:subClassOf schema:Course .

# Core User Relations object property;
# School-Member Relationships
drk:hasMember a rdf:Property, owl:DatatypeProperty ;
    rdfs:label "has member"@en ;
    rdfs:isDefinedBy drk: ;
    rdfs:domain :MusicEducationSchool ;
    rdfs:range :MusicSchoolMember ;
    rdfs:comment "Connects a music school to its members (teachers and students)."@en ;
    skos:definition "Core relationship that links a music education institution to individuals who are part of the school community."@en .

drk:memberOfSchool a rdf:Property, owl:ObjectProperty ;
    rdfs:label "member of school"@en ;
    rdfs:isDefinedBy drk: ;
    rdfs:domain :MusicSchoolMember ;
    rdfs:range :MusicEducationSchool ;
    owl:inverseOf drk:hasMember ;
    rdfs:comment "Indicates which school a member belongs to."@en ;
    skos:definition "Relationship connecting an individual to their affiliated music education institution."@en .

# Teacher-School Relationships
drk:teachesAtSchool a rdf:Property, owl:ObjectProperty ;
    rdfs:label "teaches at school"@en ;
    rdfs:isDefinedBy drk: ;
    rdfs:domain :MusicTeacher ;
    rdfs:range :MusicEducationSchool ;
    rdfs:subPropertyOf drk:memberOfSchool ;
    rdfs:comment "Links a teacher to the school where they provide instruction."@en ;
    skos:definition "Specialized relationship that connects music instructors to their affiliated educational institution."@en .

# Student-School Relationships
drk:studiesAtSchool a rdf:Property, owl:ObjectProperty ;
    rdfs:label "studies at school"@en ;
    rdfs:isDefinedBy drk: ;
    rdfs:domain :MusicStudent ;
    rdfs:range :MusicEducationSchool ;
    rdfs:subPropertyOf drk:memberOfSchool ;
    rdfs:comment "Links a student to the school where they receive music education."@en ;
    skos:definition "Specialized relationship that connects music learners to their affiliated educational institution."@en .

# Teacher-Student Relationships
drk:teachesStudent a rdf:Property, owl:ObjectProperty ;
    rdfs:label "teaches student"@en ;
    rdfs:isDefinedBy drk: ;
    rdfs:domain :MusicTeacher ;
    rdfs:range :MusicStudent ;
    rdfs:comment "Connects a teacher to students they instruct."@en ;
    skos:definition "Educational relationship between an instructor and learner in the music education context."@en .

drk:hasMatch a rdf:Property, owl:ObjectProperty ;
    rdfs:label "has match"@en ;
    rdfs:isDefinedBy drk: ;
    rdfs:domain :MusicTeacher ;
    rdfs:range :MusicStudent ;
    rdfs:comment "Represents a formal pairing between a teacher and student."@en ;
    skos:definition "System-mediated connection that indicates a teacher-student match based on preferences, instruments, and genres."@en .

# Course/Lesson Relationships
drk:conductsCourse a rdf:Property, owl:ObjectProperty ;
    rdfs:label "conducts course"@en ;
    rdfs:isDefinedBy drk: ;
    rdfs:domain :MusicTeacher ;
    rdfs:range :MusicCourse ;
    rdfs:comment "Links a teacher to courses they lead or instruct."@en ;
    skos:definition "Relationship connecting an instructor to specific music instruction sessions they provide."@en .

drk:attendsCourse a rdf:Property, owl:ObjectProperty ;
    rdfs:label "attends course"@en ;
    rdfs:isDefinedBy drk: ;
    rdfs:domain :MusicStudent ;
    rdfs:range :MusicCourse ;
    rdfs:comment "Links a student to courses they participate in."@en ;
    skos:definition "Relationship connecting a learner to specific music instruction sessions they receive."@en .

# Instrument Relationships
drk:teachesInstrument a rdf:Property, owl:ObjectProperty ;
    rdfs:label "teaches instrument"@en ;
    rdfs:isDefinedBy drk: ;
    rdfs:domain :MusicTeacher ;
    rdfs:range :InstrumentVocabulary ;
    rdfs:comment "Indicates which instruments a teacher provides instruction for."@en ;
    skos:definition "Skill-based relationship connecting an instructor to musical instruments they are qualified to teach."@en .

drk:learnsInstrument a rdf:Property, owl:ObjectProperty ;
    rdfs:label "learns instrument"@en ;
    rdfs:isDefinedBy drk: ;
    rdfs:domain :MusicStudent ;
    rdfs:range :InstrumentVocabulary  ;
    rdfs:comment "Indicates which instruments a student is learning to play."@en ;
    skos:definition "Skill-acquisition relationship connecting a student to musical instruments they are studying."@en .

drk:offersInstrument a rdf:Property, owl:ObjectProperty ;
    rdfs:label "offers instrument"@en ;
    rdfs:isDefinedBy drk: ;
    rdfs:domain :MusicEducationSchool ;
    rdfs:range :MusicalInstrumentType ;
    rdfs:comment "Indicates which instruments are taught at a music school."@en ;
    skos:definition "Curriculum-based relationship connecting an educational institution to musical instruments included in their programs."@en .

# Genre Relationships
drk:teachesGenre a rdf:Property, owl:ObjectProperty ;
    rdfs:label "teaches genre"@en ;
    rdfs:isDefinedBy drk: ;
    rdfs:domain :MusicTeacher ;
    rdfs:range :GenreVocabulary   ;
    rdfs:comment "Indicates which music genres a teacher specializes in teaching."@en ;
    skos:definition "Expertise-based relationship connecting an instructor to music styles they are qualified to teach."@en .

drk:learnsGenre a rdf:Property, owl:ObjectProperty ;
    rdfs:label "learns genre"@en ;
    rdfs:isDefinedBy drk: ;
    rdfs:domain :MusicStudent ;
    rdfs:range :GenreVocabulary ;
    rdfs:comment "Indicates which music genres a student is interested in learning."@en ;
    skos:definition "Interest-based relationship connecting a student to music styles they wish to study."@en .

drk:offersGenre a rdf:Property, owl:ObjectProperty ;
    rdfs:label "offers genre"@en ;
    rdfs:isDefinedBy drk: ;
    rdfs:domain :MusicEducationSchool ;
    rdfs:range :GenreVocabulary ;
    rdfs:comment "Indicates which music genres are taught at a music school."@en ;
    skos:definition "Curriculum-based relationship connecting an educational institution to music styles included in their programs."@en .

# Integration Relationships
drk:hasIntegration a rdf:Property, owl:ObjectProperty ;
    rdfs:label "has integration"@en ;
    rdfs:isDefinedBy drk: ;
    rdfs:domain :MusicTeacher, :MusicEducationSchool ;
    rdfs:range schema:ServiceChannel ;
    rdfs:comment "Links teachers or schools to external service integrations."@en ;
    skos:definition "Connection that enables interoperability with third-party music education platforms or tools."@en .

# Schedule Relationships
drk:hasSchedule a rdf:Property, owl:ObjectProperty ;
    rdfs:label "has schedule"@en ;
    rdfs:isDefinedBy drk: ;
    rdfs:domain :MusicTeacher, :MusicStudent ;
    rdfs:range time:TemporalEntity ;
    rdfs:comment "Links teachers or students to their availability schedules."@en ;
    skos:definition "Time management relationship that defines when an individual is available for music instruction."@en .

# School Language Property
drk:schoolLanguage a rdf:Property, owl:ObjectProperty ;
    rdfs:label "school language"@en ;
    rdfs:isDefinedBy drk: ;
    rdfs:domain :MusicEducationSchool ;
    rdfs:range :LanguageVocabulary ;
    rdfs:comment "Primary language used by the music school."@en ;
    skos:definition "Language setting that defines the default communication language for the school."@en .

# Match Notification Property
drk:matchNotificationType a rdf:Property, owl:ObjectProperty ;
    rdfs:label "match notification type"@en ;
    rdfs:isDefinedBy drk: ;
    rdfs:domain :MusicEducationSchool ;
    rdfs:range :NotificationRoutingVocabulary ;
    rdfs:comment "Defines how teacher-student match notifications are routed."@en ;
    skos:definition "Configuration setting that determines whether match notifications are sent to school administrators or directly to teachers."@en .

# Member Language Property
drk:preferredLanguage a rdf:Property, owl:ObjectProperty ;
    rdfs:label "preferred language"@en ;
    rdfs:isDefinedBy drk: ;
    rdfs:domain :MusicSchoolMember ;
    rdfs:range :LanguageVocabulary ;
    rdfs:comment "The language a member prefers for communication."@en ;
    skos:definition "User-selected language preference for system interactions."@en .

# Member Role Property
drk:userRole a rdf:Property, owl:ObjectProperty ;
    rdfs:label "user role"@en ;
    rdfs:isDefinedBy drk: ;
    rdfs:domain :MusicSchoolMember ;
    rdfs:range :RoleVocabulary ;
    rdfs:comment "The system role assigned to the member."@en ;
    skos:definition "Access control categorization that determines permissions and capabilities."@en .

# Member Gender Property
drk:gender a rdf:Property, owl:ObjectProperty ;
    rdfs:label "gender"@en ;
    rdfs:isDefinedBy drk: ;
    rdfs:domain :MusicSchoolMember ;
    rdfs:range :GenderVocabulary ;
    rdfs:comment "The gender identity of the member."@en ;
    skos:definition "Self-identified gender used for profile information and matching."@en .

# Teaching Location Types
drk:teachingLocationTypes a rdf:Property, owl:ObjectProperty ;
    rdfs:label "teaching location types"@en ;
    rdfs:isDefinedBy drk: ;
    rdfs:domain :MusicTeacher ;
    rdfs:range :LocationVocabulary ;
    rdfs:comment "Where a teacher conducts lessons."@en ;
    skos:definition "Setting that indicates whether a teacher offers online lessons, in-person lessons, or both."@en .

# Student Experience Level Preference
drk:preferredStudentLevel a rdf:Property, owl:ObjectProperty ;
    rdfs:label "preferred student level"@en ;
    rdfs:isDefinedBy drk: ;
    rdfs:domain :MusicTeacher ;
    rdfs:range :ExperienceLevelVocabulary ;
    rdfs:comment "The experience levels a teacher prefers to teach."@en ;
    skos:definition "Teacher-specified preference for student skill levels."@en .

# Student Experience Level
drk:experienceLevel a rdf:Property, owl:ObjectProperty ;
    rdfs:label "experience level"@en ;
    rdfs:isDefinedBy drk: ;
    rdfs:domain :MusicStudent ;
    rdfs:range :ExperienceLevelVocabulary ;
    rdfs:comment "The current experience level of the student."@en ;
    skos:definition "Classification of a student's proficiency in their musical studies."@en .

# Preferred Learning Location
drk:preferredLearningLocation a rdf:Property, owl:ObjectProperty ;
    rdfs:label "preferred learning location"@en ;
    rdfs:isDefinedBy drk: ;
    rdfs:domain :MusicStudent ;
    rdfs:range :LocationVocabulary ;
    rdfs:comment "Where a student prefers to take lessons."@en ;
    skos:definition "Setting that indicates whether a student prefers online lessons, in-person lessons, or both."@en .
# Lesson Place Property
drk:lessonPlace a rdf:Property, owl:ObjectProperty ;
    rdfs:label "lesson place"@en ;
    rdfs:isDefinedBy drk: ;
    rdfs:domain :MusicCourse ;
    rdfs:range :LocationVocabulary ;
    rdfs:comment "The location type for the music lesson."@en ;
    skos:definition "Setting that specifies whether the lesson takes place online or in person."@en .

# Lesson Type Property
drk:lessonType a rdf:Property, owl:ObjectProperty ;
    rdfs:label "lesson type"@en ;
    rdfs:isDefinedBy drk: ;
    rdfs:domain :MusicCourse ;
    rdfs:range :LessonTypeVocabulary ;
    rdfs:comment "The type of instructional session."@en ;
    skos:definition "Classification indicating whether the lesson is a trial, single session, or recurring lesson."@en .

# Lesson Duration Property
drk:lessonDuration a rdf:Property, owl:ObjectProperty ;
    rdfs:label "lesson duration"@en ;
    rdfs:isDefinedBy drk: ;
    rdfs:domain :MusicCourse ;
    rdfs:range :DurationVocabulary ;
    rdfs:comment "The scheduled duration of the lesson."@en ;
    skos:definition "Time length setting for the instructional session."@en .

# Lesson Status Property
drk:lessonStatus a rdf:Property, owl:ObjectProperty ;
    rdfs:label "lesson status"@en ;
    rdfs:isDefinedBy drk: ;
    rdfs:domain :MusicCourse ;
    rdfs:range :StatusVocabulary ;
    rdfs:comment "The current status of the lesson."@en ;
    skos:definition "State indicator for the lesson's lifecycle stage."@en .




# MusicEducationSchool Data Properties
drk:schoolName a owl:DatatypeProperty ;
    rdfs:label "school name"@en ;
    rdfs:isDefinedBy drk: ;
    rdfs:domain :MusicEducationSchool ;
    rdfs:range xsd:string ;
    rdfs:comment "The official name of the music education institution."@en ;
    skos:definition "Primary identifier for a music school as displayed to users and in communications."@en .

drk:isConfirmed a owl:DatatypeProperty ;
    rdfs:label "is confirmed"@en ;
    rdfs:isDefinedBy drk: ;
    rdfs:domain :MusicEducationSchool ;
    rdfs:range xsd:boolean ;
    rdfs:comment "Indicates whether the school's registration has been confirmed."@en ;
    skos:definition "Status flag that determines if a music school has completed the basic registration process."@en .

drk:isValidated a owl:DatatypeProperty ;
    rdfs:label "is validated"@en ;
    rdfs:isDefinedBy drk: ;
    rdfs:domain :MusicEducationSchool ;
    rdfs:range xsd:boolean ;
    rdfs:comment "Indicates whether the school has been officially validated."@en ;
    skos:definition "Status flag that determines if a music school has been verified by system administrators."@en .

drk:teachersAmount a owl:DatatypeProperty ;
    rdfs:label "teachers amount"@en ;
    rdfs:isDefinedBy drk: ;
    rdfs:domain :MusicEducationSchool ;
    rdfs:range xsd:integer ;
    rdfs:comment "The number of teachers associated with the school."@en ;
    skos:definition "Counter that tracks the total number of instructors affiliated with a music education institution."@en .

drk:studentsAmount a owl:DatatypeProperty ;
    rdfs:label "students amount"@en ;
    rdfs:isDefinedBy drk: ;
    rdfs:domain :MusicEducationSchool ;
    rdfs:range xsd:integer ;
    rdfs:comment "The number of students enrolled in the school."@en ;
    skos:definition "Counter that tracks the total number of learners enrolled in a music education institution."@en .

drk:schoolLanguage a owl:DatatypeProperty ;
    rdfs:label "school language"@en ;
    rdfs:isDefinedBy drk: ;
    rdfs:domain :MusicEducationSchool ;
    rdfs:range xsd:string ;
    rdfs:comment "The primary language used for instruction at the school."@en ;
    skos:definition "Primary communication language used by the music education institution for teaching and administration."@en .

drk:matchNotificationType a owl:DatatypeProperty ;
    rdfs:label "match notification type"@en ;
    rdfs:isDefinedBy drk: ;
    rdfs:domain :MusicEducationSchool ;
    rdfs:range xsd:string ;
    rdfs:comment "Specifies how match notifications are handled at the school."@en ;
    skos:definition "Configuration that determines the routing of teacher-student matching notifications within the institution."@en .

drk:schoolWebsite a owl:DatatypeProperty ;
    rdfs:label "school website"@en ;
    rdfs:isDefinedBy drk: ;
    rdfs:domain :MusicEducationSchool ;
    rdfs:range xsd:string ;
    rdfs:comment "The URL of the school's official website."@en ;
    skos:definition "Web address where additional information about the music education institution can be found."@en .

drk:schoolDescription a owl:DatatypeProperty ;
    rdfs:label "school description"@en ;
    rdfs:isDefinedBy drk: ;
    rdfs:domain :MusicEducationSchool ;
    rdfs:range xsd:string ;
    rdfs:comment "A description of the music school and its offerings."@en ;
    skos:definition "Narrative text that explains the music school's mission, approach, and educational philosophy."@en .

drk:postalCode a owl:DatatypeProperty ;
    rdfs:label "postal code"@en ;
    rdfs:isDefinedBy drk: ;
    rdfs:domain :MusicEducationSchool, :MusicSchoolMember ;
    rdfs:range xsd:string ;
    rdfs:comment "The postal code of the physical location."@en ;
    skos:definition "Geographic identifier used for mail delivery and location-based services."@en .

drk:city a owl:DatatypeProperty ;
    rdfs:label "city"@en ;
    rdfs:isDefinedBy drk: ;
    rdfs:domain :MusicEducationSchool, :MusicSchoolMember ;
    rdfs:range xsd:string ;
    rdfs:comment "The city where the entity is located."@en ;
    skos:definition "Geographic location identifier for urban area association."@en .

drk:phoneNumber a owl:DatatypeProperty ;
    rdfs:label "phone number"@en ;
    rdfs:isDefinedBy drk: ;
    rdfs:domain :MusicEducationSchool, :MusicSchoolMember ;
    rdfs:range xsd:string ;
    rdfs:comment "Contact phone number for the entity."@en ;
    skos:definition "Telecommunication identifier used for direct voice communication."@en .

drk:priceRange a owl:DatatypeProperty ;
    rdfs:label "price range"@en ;
    rdfs:isDefinedBy drk: ;
    rdfs:domain :MusicEducationSchool, :MusicTeacher ;
    rdfs:range xsd:string ;
    rdfs:comment "The range of prices for lessons or services."@en ;
    skos:definition "Financial information detailing the minimum and maximum cost of music instruction services."@en .







# School-Member Relations
:hasSchoolAssociation
   a owl:ObjectProperty ;
   rdfs:domain :MusicEducationSchool ;
   rdfs:range [
       owl:unionOf (
           :MusicInstructor
           :MusicLearner
           :MusicSchoolDocument
       )
   ] ;
   skos:definition "Links a school to its associated members and documents."@en .

# Instruction Relations
:hasInstructionParticipant
   a owl:ObjectProperty ;
   rdfs:domain :MusicInstruction ;
   rdfs:range [
       owl:unionOf (
           :MusicInstructor
           :MusicLearner
       )
   ] ;
   skos:definition "Connects an instruction session to its participants."@en .

:hasInstructionSchedule
   a owl:ObjectProperty ;
   rdfs:domain :MusicInstruction ;
   rdfs:range :InstructionSchedule ;
   skos:definition "Links instruction to its scheduled time slot."@en .

# Integration Relations
:hasServiceIntegration
   a owl:ObjectProperty ;
   rdfs:domain [
       owl:unionOf (
           :MusicEducationSchool
           :MusicInstructor
       )
   ] ;
   rdfs:range :ExternalMusicService ;
   skos:definition "Connects schools/instructors to external services."@en .

# Member Confirmation Relations
:hasMemberConfirmation
   a owl:ObjectProperty ;
   rdfs:domain :MusicSchoolMember ;
   rdfs:range :MusicSchoolMemberConfirmationType ; #Enum
   skos:definition "Links members to their verification events."@en .

# Document Relations
:hasAssociatedDocument
   a owl:ObjectProperty ;
   rdfs:domain [
       owl:unionOf (
           :MusicEducationSchool
           :MusicSchoolMember
       )
   ] ;
   rdfs:range :MusicSchoolDocument ;
   skos:definition "Connects entities to their related documents."@en .

# Musical Relations
:hasMusicalProfile
   a owl:ObjectProperty ;
   rdfs:domain [
       owl:unionOf (
           :MusicInstructor
           :MusicLearner
           :MusicEducationSchool
       )
   ] ;
   rdfs:range [
       owl:unionOf (
           :MusicalInstrumentType
           :MusicGenreCategory
       )
   ] ;
   skos:definition "Links entities to their musical preferences and capabilities."@en .

# Schedule-Related
:hasAvailabilitySchedule
    a owl:ObjectProperty ;
    rdfs:domain [
        owl:unionOf (:MusicInstructor :MusicLearner)
    ] ;
    rdfs:range :InstructionSchedule ;
    skos:definition "Links members to their availability schedules."@en .

# Matching-Related
:hasTeacherStudentMatch
    a owl:ObjectProperty ;
    rdfs:domain :TeacherStudentPairing ;
    rdfs:range [
        owl:unionOf (:MusicInstructor :MusicLearner)
    ] ;
