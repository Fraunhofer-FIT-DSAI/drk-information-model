# MusicGenreCategory Vocabulary Enumeration 
:GenreVocabulary 
    a skos:ConceptScheme ;
    rdfs:isDefinedBy drk: ;
    rdfs:label "Music Genres"@en ;
    skos:definition "Enumeration of music styles and genres"@en ;
    owl:equivalentClass mo:Genre .

# Concepts based on the genres https://app.musiq.me/
:Blues 
    a skos:Concept ;
    skos:inScheme :GenreVocabulary ;
    skos:prefLabel "Blues"@en .

:Electronic 
    a skos:Concept ;
    skos:inScheme :GenreVocabulary ;
    skos:prefLabel "Electronic"@en .

:HipHop 
    a skos:Concept ;
    skos:inScheme :GenreVocabulary ;
    skos:prefLabel "Hip Hop"@en .

:Jazz 
    a skos:Concept ;
    skos:inScheme :GenreVocabulary ;
    skos:prefLabel "Jazz"@en .

:Classical 
    a skos:Concept ;
    skos:inScheme :GenreVocabulary ;
    skos:prefLabel "Classical"@en .

# Add other genres as needed ...>

# InstrumentVocabulary Enumeration
:InstrumentVocabulary 
    a skos:ConceptScheme ;
    rdfs:isDefinedBy drk: ;
    rdfs:label "Musical Instruments"@en ;
    skos:definition "Enumeration of musical instruments"@en ;
    owl:equivalentClass dbo:MusicalInstrument ;
    owl:equivalentClass mo:Instrument .

# Concepts based on the instruments https://app.musiq.me/
:BassGuitar 
    a skos:Concept ;
    skos:inScheme :InstrumentVocabulary ;
    skos:prefLabel "Bass Guitar"@en .

:Flute 
    a skos:Concept ;
    skos:inScheme :InstrumentVocabulary ;
    skos:prefLabel "Flute"@en .

:Vocals 
    a skos:Concept ;
    skos:inScheme :InstrumentVocabulary ;
    skos:prefLabel "Vocals"@en .

:Guitar 
    a skos:Concept ;
    skos:inScheme :InstrumentVocabulary ;
    skos:prefLabel "Guitar"@en .

:Keyboard 
    a skos:Concept ;
    skos:inScheme :InstrumentVocabulary ;
    skos:prefLabel "Keyboard"@en .

