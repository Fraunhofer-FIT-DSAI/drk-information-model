    <owl:Ontology rdf:about="http://w3id.org/drk/ontology/">
        <owl:versionIRI rdf:resource="http://w3id.org/drk/ontology/1.0.1"/>
        <ns:license rdf:resource="http://creativecommons.org/licenses/by/4.0/"/>
        <terms:audience xml:lang="en">Cultural institutions</terms:audience>
        <terms:audience xml:lang="en">Researchers</terms:audience>
        <terms:audience xml:lang="en">Theater professionals</terms:audience>
        <terms:coverage xml:lang="en">Global</terms:coverage>
        <terms:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-10-22</terms:created>
        <terms:description xml:lang="en">
        The DRK (Datenraum Kultur) ontology provides a comprehensive vocabulary for describing theatrical productions and cultural events.
        It includes concepts for:
        - Theatrical productions and events
        - Characters, roles and performances
        - Venues and their accessibility
        - Event scheduling and management

        The ontology is designed to be extensible and interoperable with other cultural heritage vocabularies.
    </terms:description>
        <terms:hasVersion>1.0.1</terms:hasVersion>
        <terms:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2025-04-24</terms:issued>
        <terms:language rdf:resource="https://w3id.org/drk/vocabs/languages-iso639-1#de"/>
        <terms:language rdf:resource="https://w3id.org/drk/vocabs/languages-iso639-1#en"/>
        <terms:license rdf:resource="http://creativecommons.org/licenses/by/4.0/"/>
        <terms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2025-04-24</terms:modified>
        <terms:publisher rdf:nodeID="genid1"/>
        <terms:rights xml:lang="en">This ontology is distributed under Creative Commons Attribution 4.0 International License</terms:rights>
        <terms:subject xml:lang="en">Cultural Events</terms:subject>
        <terms:subject xml:lang="en">Performing Arts</terms:subject>
        <terms:subject xml:lang="en">Play Production</terms:subject>
        <terms:subject xml:lang="en">Theater</terms:subject>
        <terms:title xml:lang="en">Culture Dataspace (DRK) Ontology</terms:title>
        <terms:title xml:lang="de">Datenraum Kultur (DRK) Ontologie</terms:title>
        <bibo:doi>10.5281/zenodo.15294907</bibo:doi>
        <vann:example rdf:resource="https://github.com/Fraunhofer-FIT-DSAI/drk-information-model/tree/main/examples"/>
        <vann:preferredNamespacePrefix>drk</vann:preferredNamespacePrefix>
        <vann:preferredNamespaceUri>http://w3id.org/drk/ontology/</vann:preferredNamespaceUri>
        <rdfs:comment xml:lang="en">The DRK ontology is designed for the cultural dataspace, providing extensible semantic concepts, properties, and their interrelationships for modeling metadata related to theaters, theatrical productions, and their showtimes.</rdfs:comment>
        <rdfs:label xml:lang="en">Culture Dataspace (DRK) Ontology</rdfs:label>
        <rdfs:label xml:lang="de">Datenraum Kultur (DRK) Ontologie</rdfs:label>
        <rdfs:seeAlso rdf:resource="https://github.com/Fraunhofer-FIT-DSAI/drk-information-model"/>
        <owl:priorVersion rdf:resource="http://w3id.org/drk/ontology/1.0.0"/>
        <owl:versionInfo>1.0.1</owl:versionInfo>
        <schema:author>Christoph Lange-Bever</schema:author>
        <schema:author>Daham Mohammed Mustafa</schema:author>
        <schema:author>Peter Retzlaff</schema:author>
        <schema:author>Rohit A. Deshmukh</schema:author>
    </owl:Ontology>
